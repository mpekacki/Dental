            <style> 
             tr{page-break-inside:avoid; page-break-after:auto}
                table{
                border-style: solid;
                border-color: #000000;
                background-color: #ffffff;
                border-collapse:collapse;
                border-width: 2px;
                }
                .gridtable td {
                border-width: 2px;
                padding: 8px;
                border-style: solid;
                border-color: #000000;
                background-color: #ffffff;
                border-collapse:collapse;
                border-width: 2px;
